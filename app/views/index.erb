<div class="container">
  <a id="question-button" href="/questions/new">Ask Question</a>
</header>


<div class='question-container'>
<h2>Questions</h2>

<div id="question-list">
  <% @questions.each do |question| %>

      <div class="question-title"><a href="/questions/<%= question.id%>"><%= question.title %></a>
        <div class="votes">
          <span class="vote-count"><%= question.votes.count %></span>
          <div class="vote-count-subtitle">votes</div>
        </div>
        <div class="answers">
          <span class="answer-count"><%= question.answers.count %></span>
          <div class="answer-count-subtitle">answers</div>
        </div>
        </div>
      </div>

  <% end %>
</div>
</div>

</div>
